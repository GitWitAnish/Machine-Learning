   "source": [
    "import numpy as np\n",
    "\n",
    "# dummy data\n",
    "X = np.array([[1, 2],\n",
    "              [3, 4]]).T  # shape (2,2)\n",
    "\n",
    "W = np.array([[0.5, -0.6],\n",
    "              [0.1, 0.8]])\n",
    "b = np.zeros((2,1))\n",
    "\n",
    "Z = W @ X + b\n",
    "A = np.maximum(0, Z)\n",
    "\n",
    "print(\"Z:\\n\", Z)\n",
    "print(\"ReLU output A:\\n\", A)\n"
   ]
  },
